[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[user]
	name = Rebecca Turner
	email = 637275@gmail.com
[push]
	default = simple
	followTags = true
[credential]
	helper = manager
	modalPrompt = true
[core]
	editor = vim
	autocrlf = false
	excludesfile = ~/.gitignore_global
	attributesfile = ~/.gitattributes_global
	eol = lf
[merge]
	conflictstyle = diff3
[diff]
	algorithm = minimal
[difftool]
	prompt = false
[rerere]
	enabled = true
[alias]
	last = log -1 HEAD
	graph = log --format=\"%C(cyan)%H%Creset%n%C(red) %an %C(reset)%s %ar\" --graph
	st = status --short --branch
	a = add . --verbose
	changes = diff --cached
	ch = diff --cached
	abandon = checkout HEAD -- 
[hub]
	protocol = https
[branch]
	autosetupmerge = always
	autosetuprebase = always
[branch.master]
	remote = origin
